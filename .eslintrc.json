{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "indent": [
            "error",
            2
        ]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}